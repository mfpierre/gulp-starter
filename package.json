{
  "name": "gulp-starter",
  "description": "Gulp starter customized with Less pre-processor and Jade templates",
  "repository": {
    "type": "git",
    "url": "git://github.com:mfpierre/gulp-starter.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "uglifyify"
    ]
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.7.0",
    "browserify": "~6.3.2",
    "browserify-shim": "~3.8.0",
    "coffeeify": "~1.0.0",
    "gulp": "^3.8.10",
    "gulp-changed": "^1.0.0",
    "gulp-imagemin": "^2.0.0",
    "gulp-notify": "^2.0.1",
    "gulp-util": "^3.0.1",
    "pretty-hrtime": "~0.2.2",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "~2.1.1",
    "gulp-jade": "~0.9.0",
    "gulp-less": "~1.3.6",
    "uglifyify": "~2.6.0"
  },
  "dependencies": {
    "jquery": "~2.1.1"
  }
}
