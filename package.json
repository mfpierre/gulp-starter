{
  "name": "gulp-starter",
  "description": "Gulp starter customized with Less pre-processor and Jade templates",
  "repository": {
    "type": "git",
    "url": "git://github.com:mfpierre/gulp-starter.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "uglifyify"
    ]
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~2.18.2",
    "browserify": "~13.1.1",
    "browserify-shim": "~3.8.12",
    "coffeeify": "~2.0.1",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-util": "^3.0.7",
    "pretty-hrtime": "~1.0.3",
    "require-dir": "^0.3.1",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.7.0",
    "gulp-jade": "~1.1.0",
    "gulp-less": "~3.3.0",
    "uglifyify": "~3.0.4"
  },
  "dependencies": {
    "jquery": "~3.1.1"
  }
}
